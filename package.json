{
  "name": "broccoli-iced-coffee",
  "version": "0.2.0",
  "description": "IcedCoffeeScript compiler for broccoli",
  "main": "index.js",
  "scripts": {
    "lint": "jshint {,test/}*.js --reporter node_modules/jshint-stylish/stylish",
    "build": "rimraf test/actual && broccoli build test/actual",
    "pretest": "npm run-script lint && npm run-script build",
    "test": "mocha --reporter spec test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/broccoli-iced-coffee.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/broccoli-iced-coffee/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/shinnn/broccoli-iced-coffee/issues"
  },
  "homepage": "https://github.com/shinnn/broccoli-iced-coffee",
  "keywords": [
    "broccoli-plugin",
    "iced-coffee-script",
    "coffeescript",
    "language",
    "compiler",
    "transpiler",
    "altjs"
  ],
  "dependencies": {
    "broccoli-filter": "^0.1.6",
    "iced-coffee-script": "^1.7.1-b"
  },
  "devDependencies": {
    "broccoli": "^0.9.0",
    "jshint": "^2.5.0",
    "jshint-stylish": "^0.1.5",
    "mocha": "^1.18.2",
    "q": "^2.0.1",
    "rimraf": "^2.2.6"
  }
}
